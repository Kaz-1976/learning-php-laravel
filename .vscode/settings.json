{
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "blade.format.enable": true,
  "bladeFormatter.format.indentSize": 4,
  "bladeFormatter.format.indentInnerHtml": true,
  "bladeFormatter.format.noMultipleEmptyLines": false,
  "css.completion.completePropertyWithSemicolon": false,
  "css.completion.triggerPropertyValueCompletion": false,
  "css.format.spaceAroundSelectorSeparator": true,
  "css.validate": true,
  "editor.defaultFormatter": "shufo.vscode-blade-formatter",
  "html.format.templating": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.experimental.configFile": "src/tailwind.config.js",
  "tailwindCSS.hovers": true,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "notebook.defaultFormatter": null,
}